        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/DefeaterDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/AggregationDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/SelectionDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/ChoiceDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/BinaryDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/BooleanDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/InteractiveDecision"/>
        <xs:enumeration value="http://ontology.mayo.edu/ontologies/decisionmodels/ActionableDecision"/>
